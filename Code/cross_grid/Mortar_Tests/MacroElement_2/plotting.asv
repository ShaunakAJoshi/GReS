% plotting the results of the inf-sup test on the macroelement
h = [1/4;1/8;1/16;1/32];
s_coarse = load('slaveCoarse.mat');
s_fine = load('slaveFine.mat');
p0 = s_coarse.p0;
dual = s_coarse.dual;
figure(1)
plot(log(h),log(p0),'LineWidth',1.2,'r-s');
hold on
plot(log(h),log(dual),'LineWidth',1.2,'k-^');
p0 = s_fine.p0;
dual = s_fine.dual;
plot(log(h),log(p0),'LineWidth',1.2,'r--s');
plot(log(h),log(dual),'LineWidth',1.2,'k--^');




