[dual_tx,dual_ty,anal_dual_tx,anal_dual_ty] = RunPatchTest('dual',10,0);
[standard_tx,standard_ty,anal_standard_tx,anal_standard_ty] = RunPatchTest('standard',10,0);

%% plotting
xNode = linspace(0,1,numel(dual_tx));
xCell = linspace(0+0.5/numel(anal_standard_ty),1-0.5/numel(anal_standard_ty),numel(anal_standard_ty));
tiledlayout(1,2)
nexttile
plot(xNode,dual_tx,'k','LineWidth',1)
hold on
plot(xNode,standard_tx,'r-s','LineWidth',1)
plot(xCell,anal_dual_tx,'b--o','LineWidth',1)
xlabel('x (m)')
ylabel('t_x (kPa)')
legend('dual','standard','anal')

nexttile
plot(xNode,dual_ty,'k-s','LineWidth',1)
hold on
plot(xNode,standard_ty,'r-s','LineWidth',1)
plot(xCell,anal_dual_ty,'b--o','LineWidth',1)
xlabel('x (m)')
ylabel('t_y (kPa)')
legend('dual','standard','anal')